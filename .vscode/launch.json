    "version": "0.2.0",
    "configurations": [
        {   "name": "debug ram actor",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "$@",
                "--exp_name",
                "ram_insertion",
                "--checkpoint_path=../../experiments/ram_insertion/first_run",
                "--actor"
            ],
            "cwd": "examples/experiments/ram_insertion" ,
            "console": "integratedTerminal",
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".1"
            },
        },
        {   "name": "debug resistor align actor",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "$@",
                "--exp_name",
                "resistor_align",
                "--checkpoint_path=examples/experiments/resistor_align/first_run",
                "--actor",
                "--eval_checkpoint_step",
                "5000",
                "--eval_n_trajs",
                "1"
            ],
            "console": "integratedTerminal",
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".1"
            },
        },
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "debug usb",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--exp_name",
                "usb_pickup_insertion",
            ],
            "cwd": "examples",
            "console": "integratedTerminal"
        },
        {
            "name": "debug ram",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--exp_name",
                "ram_insertion",
            ],
            // "cwd": "examples",
            "console": "integratedTerminal"
        },
        {
            "name": "debug resistor",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--exp_name",
                "resistor_insertion",
            ],
            // "cwd": "examples",
            "console": "integratedTerminal"
        },
        {
            "name": "debug align resistor",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--exp_name",
                "resistor_align",
            ],
            // "cwd": "examples",
            "console": "integratedTerminal"
        },
        {
            "name": "debug push_in resistor",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "--exp_name",
                "resistor_push_in",
            ],
            // "cwd": "examples",
            "console": "integratedTerminal"
        },
        {   "name": "debug multi_actor",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "args": [
                "$@",
                "--exp_names",
                "resistor_align,resistor_push_in",
                "--checkpoint_paths",
                "examples/experiments/resistor_align/first_run,examples/experiments/resistor_push_in/first_run ",
                "--eval_checkpoint_steps",
                "5000,10000",
                "--eval_n_trajs",
                "10"
            ],
            "console": "integratedTerminal",
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".1"
            },
        },

    ]
}